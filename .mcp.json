{
  "mcpServers": {
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide",
        "--project",
        "."
      ],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@2.1.2"
      ],
      "env": {}
    },
    "microsoftdocs-mcp": {
      "type": "http",
      "url": "https://learn.microsoft.com/api/mcp"
    },
    "mcp-windbg": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/svnscha/mcp-windbg@v0.12.2",
        "mcp-windbg"
      ],
      "env": {}
    }
  }
}
