{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/InferenceGateway/WebApi/bin/Debug/net10.0/Synaxis.InferenceGateway.WebApi.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/InferenceGateway/WebApi",
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:7149;http://localhost:5042"
      },
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}"
      }
    }
  ]
}