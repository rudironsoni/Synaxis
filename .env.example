POSTGRES_DB=synaxis
POSTGRES_USER=synaxis
# PostgreSQL - Generate strong passwords
# Example: openssl rand -base64 32
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Redis - Generate strong password
# Example: openssl rand -base64 32
REDIS_PASSWORD=your_secure_redis_password_here

# ===========================================
# Qdrant Vector Database
# ===========================================
# Qdrant API Key - generate a strong random key
# Example: openssl rand -base64 32 | tr -d '=+/\n' | cut -c1-48
QDRANT_API_KEY=QDRANT_your_generated_api_key_here

# ===========================================
# Monitoring (Optional - use --profile monitoring)
# ===========================================
GRAFANA_ADMIN_USER=admin
# Auto-generated secure password
GRAFANA_ADMIN_PASSWORD=grafana_$(openssl rand -base64 16 | tr -d '\n')

# ===========================================
# Token Optimization Settings
# ===========================================
TOKEN_OPTIMIZATION_ENABLED=true
TOKEN_OPTIMIZATION_SIMILARITY_THRESHOLD=0.85
TOKEN_OPTIMIZATION_CACHE_TTL_HOURS=24

# JWT Secret - must be at least 32 characters
# Generate with: openssl rand -base64 32
JWT_SECRET=your-super-secret-jwt-key-minimum-32-chars-long

PGADMIN_DEFAULT_EMAIL=admin@example.com
# pgAdmin - Default password for web interface
PGADMIN_DEFAULT_PASSWORD=your_pgadmin_password_here

# Groq API Key - starts with gsk_
# Get from: https://console.groq.com/keys
GROQ_API_KEY=gsk_your_groq_api_key_here
# Cohere API Key
# Get from: https://dashboard.cohere.com/api-keys
COHERE_API_KEY=your_cohere_api_key_here
# Cloudflare Workers AI
# Get from: https://dash.cloudflare.com/profile/api-tokens
CLOUDFLARE_API_KEY=your_cloudflare_api_token
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
# Google Gemini API Key
# Get from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
# OpenRouter API Key
# Get from: https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-v1-your_openrouter_key_here
# DeepSeek API
# Get from: https://platform.deepseek.com/api_keys
DEEPSEEK_API_KEY=sk-your_deepseek_api_key
DEEPSEEK_API_ENDPOINT=https://api.deepseek.com/v1
# OpenAI API
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-your_openai_api_key_here
OPENAI_API_ENDPOINT=https://api.openai.com/v1
# Google Cloud Code / Antigravity
ANTIGRAVITY_PROJECT_ID=your-gcp-project-id
ANTIGRAVITY_API_ENDPOINT=https://cloudcode-pa.googleapis.com
ANTIGRAVITY_API_ENDPOINT_FALLBACK=https://daily-cloudcode-pa.googleapis.com

# Kilo Code API Key
KILOCODE_API_KEY=your_kilocode_api_key
# NVIDIA API Key
# Get from: https://build.nvidia.com/
NVIDIA_API_KEY=nvapi-your_nvidia_api_key
# Hugging Face API Token
# Get from: https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=hf_your_huggingface_token
